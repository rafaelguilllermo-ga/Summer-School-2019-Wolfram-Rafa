Cell[BoxData[
 RowBox[{"Show", "[", 
  RowBox[{
   RowBox[{"Plot", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"PosibleSolution1", ",", "PosibleSolution2"}], "}"}], ",", 
     RowBox[{"{", 
